$(":reset").click(function () {location.reload();})
        $('.shop_ids').iCheck({checkboxClass:'icheckbox_minimal-blue'});
        $('#add-shop').click(function () {
            layer.prompt({title: '请输入店铺名称', formType: 3}, function(content, index){

            	layer.confirm('确认要继续吗？', {
            		title: '这是个重要的操作',
				  	btn: ['确认','取消']
				}, function(){
				  layer.msg('的确很重要', {icon: 1});
				});
                var load1 = layer.load(2, {shade: [0.7,'#fff'], time: 2000});
                var next = true;
                content = content.replace(/^\s*|\s*$/g,"");
                $('label.shop_ids a').each(function () {
                    if (next && $(this).text() == content) {
                        next = false;
                    }
                });
        
                if (!next) {
                    layer.close(load1);
                    layer.msg('店铺已经存在');
                    return false;
                }
        
                $.post("/brand/{$this->id}/shop",{content:content, '_token':'{$token}'},function(result){
                    layer.close(load1);
                    if (result.code) {
                        location.reload();
                    } else {
                        layer.msg(result.msg);
                    }
                });
            });
        });